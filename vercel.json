{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next@latest"
    },
    {
      "src": "apps/api/package.json",
      "use": "@vercel/node@latest"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/api/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web"
    }
  ]
}
